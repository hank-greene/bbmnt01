#*
<button onclick="soundPlayer.play('$control.audioLink')"  
        #if($!control.tooltip != "")title="$jwic.formatInp($!control.tooltip)"
        #end tabindex="-1" class="j-button $control.cssClass" 
        id="btn_${control.controlID}" 
        #if($control.enabled == false)disabled   
        #end>
        
        $control.title
        
        <input id="$control.controlID" 
               type="button" 
               data-enhanced="true" 
               value="$!control.title"
               #if(!$control.enabled) disabled #end>
        </input>
        
        
</button>
*#
<div class="#if($control.theme) ui-btn-$!control.theme #else ui-btn #end ui-input-btn
			#if($control.corners) ui-corner-all #end 
			#if($control.inline) ui-btn-inline #end 
			#if(!$control.enabled) ui-state-disabled #end
			#if($control.mini) ui-mini #end
			#if($control.shadow) ui-shadow #end 
			#if($control.iconClass) ui-icon-$control.iconClass.code ui-btn-icon-$control.iconpos #end">
  #if($control.iconPath != "")<img src="$control.iconPath"/>#end
  $!control.title
  <input onclick="soundPlayer.play('$control.audioLink')"
         id="$control.controlID" 
         type="button" 
         data-enhanced="true" 
         value="$!control.title" 
         #if(!$control.enabled) disabled #end></input>
</div>

#*
<audio id="player" src="$control.audioLink"></audio>
<div> 
  <button onclick="document.getElementById('player').play()">Play</button> 
  <button onclick="document.getElementById('player').pause()">Pause</button> 
  <button onclick="document.getElementById('player').volume += 0.1">Vol+ </button> 
  <button onclick="document.getElementById('player').volume -= 0.1">Vol- </button> 
</div>
*#